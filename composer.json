{
    "name": "Ciloe/phpqa",
    "description": "Composer Package for PHP Quality",
    "keywords": ["php", "qa", "tools", "debug"],
    "type": "metapackage",
    "license": "MIT",
    "authors": [
        {
            "name": "ESCRICH Jimmy",
            "email": "contact@escrichjimmy.fr",
            "homepage": "http://escrichjimmy.fr"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "vendor"
    },
    "require": {
        "phpunit/phpunit": "~5.0",
        "phploc/phploc": "~2.0",
        "sebastian/phpcpd": "~2.0",
        "pdepend/pdepend": "~2.0",
        "phpmd/phpmd": "~2.0",
        "squizlabs/php_codesniffer": "~2.0",
        "fabpot/php-cs-fixer": "~1.0",
        "sensiolabs/security-checker": "~3.0",
        "behat/behat": "~3.0",
        "phpdocumentor/phpdocumentor": "~2.0",
        "mayflower/php-codebrowser": "~1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
